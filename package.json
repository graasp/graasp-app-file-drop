  "license": "MIT",
  "dependencies": {
    "@graasp/apps-query-client": "github:graasp/graasp-apps-query-client.git",
    "@graasp/ui": "github:graasp/graasp-ui.git",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@uppy/core": "2.1.7",
    "@uppy/dashboard": "2.1.4",
    "@uppy/drag-drop": "2.0.6",
    "@uppy/react": "2.1.2",
    "@uppy/xhr-upload": "2.0.7",
    "clsx": "1.1.1",
    "i18next": "21.6.14",
    "immutable": "4.0.0",
    "prop-types": "15.8.1",
    "qs": "6.10.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-ga": "3.3.0",
    "react-i18next": "11.16.2",
    "react-scripts": "4.0.3",
    "react-toastify": "8.2.0"
  },
  "scripts": {
    "start": "env-cmd -f ./.env.local react-scripts start",
    "start:test": "env-cmd -f ./.env.test react-scripts start",
    "start:ci": "react-scripts -r @cypress/instrument-cra start",
    "prestart:api": "cat data/default.json > data/db.json",
    "build": "yarn build:react",
    "build:react": "react-scripts build",
    "build:bundle": "webpack --config webpack.config.js --mode=production",
    "build:local": "env-cmd -f ./.env.local yarn build",
    "build:dev": "env-cmd -f ./.env.development yarn build",
    "build:prod": "env-cmd -f ./.env.production yarn build",
    "build:version": "REACT_APP_VERSION=$CI_BRANCH env-cmd --no-override -f ./.env.prod yarn build",
    "hooks:uninstall": "node node_modules/husky/husky.js uninstall",
    "hooks:install": "node node_modules/husky/husky.js install",
    "lint": "eslint .",
    "prettier:check": "prettier --check '{src,cypress}/**/*.js'",
    "prettier:write": "prettier --write '{src,cypress}/**/*.js'",
    "test": "react-scripts test",
    "test:once": "cross-env CI=true env-cmd -f ./.env.test react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "test:ci": "run-s test:coverage report",
    "report": "cat ./coverage/lcov.info | codacy-coverage",
    "eject": "react-scripts eject",
    "release": "standard-version -a",
    "release:first": "standard-version -a --first-release",
    "release:minor": "standard-version -a --release-as minor",
    "release:major": "standard-version -a --release-as major",
    "release:patch": "standard-version -a --release-as patch",
    "cypress:open": "cypress open",
    "cypress": "concurrently \"yarn start:test\" \"wait-on http://localhost:3333 && yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome --spec \"cypress/**/*.spec.js\"",
    "postinstall": "husky install",
    "pre-commit": "yarn prettier:check && yarn lint"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-react": "7.16.7",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@cypress/code-coverage": "3.9.12",
    "@cypress/instrument-cra": "1.4.0",
    "codacy-coverage": "3.4.0",
    "concurrently": "7.1.0",
    "cross-env": "7.0.3",
    "cypress": "9.5.3",
    "cypress-file-upload": "5.0.2",
    "env-cmd": "10.1.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-react": "7.28.0",
    "glob": "7.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "interpolate-html-plugin": "4.0.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "script-ext-html-webpack-plugin": "2.1.5",
    "standard-version": "9.3.2",
    "terser-webpack-plugin": "5.3.1",
    "wait-on": "6.0.1",
    "webpack-cli": "4.9.2"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "resolutions": {
    "minimist": "1.2.6",
    "node-forge": "1.3.0",
    "nth-check": "2.0.1",
    "immer": "9.0.6",
    "glob-parent": "5.1.2",
    "browserslist": "4.16.5",
    "yargs-parser": ">=18.1.2"
  }
}
