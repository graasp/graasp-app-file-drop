    "cypress-file-upload": "5.0.2",
    "env-cmd": "10.1.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-react": "7.32.2",
    "glob": "7.2.0",
    "husky": "7.0.4",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "standard-version": "9.3.2",
    "typescript": "5.0.4",
    "wait-on": "6.0.1"
