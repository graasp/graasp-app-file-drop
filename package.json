  "scripts": {
    "start": "env-cmd -f ./.env.local react-scripts start",
    "start:test": "env-cmd -f ./.env.test react-scripts start",
    "start:ci": "react-scripts -r @cypress/instrument-cra start",
    "build": "yarn build:react",
    "build:react": "react-scripts build",
    "build:local": "env-cmd -f ./.env.local yarn build",
    "build:dev": "env-cmd -f ./.env.development yarn build",
    "build:prod": "env-cmd -f ./.env.production yarn build",
    "hooks:uninstall": "node node_modules/husky/husky.js uninstall",
    "hooks:install": "node node_modules/husky/husky.js install",
    "lint": "eslint .",
    "prettier:check": "prettier --check '{src,cypress}/**/*.{js,ts,tsx}'",
    "prettier:write": "prettier --write '{src,cypress}/**/*.{js,ts,tsx}'",
    "test": "react-scripts test",
    "test:ci": "run-s test:coverage report",
    "cypress:open": "cypress open",
    "cypress": "concurrently \"yarn start:test\" \"wait-on http://localhost:3333 && yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome --spec \"cypress/**/*.spec.js\"",
    "postinstall": "husky install",
    "pre-commit": "yarn prettier:check && yarn lint"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
